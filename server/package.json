{
  "name": "foozool-finance",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "run-p dev:mongodb dev:start",
    "dev:start": "nodemon -r dotenv/config --ext ts --exec 'node --inspect=9220 -r ts-node/register -r tsconfig-paths/register' src/index.ts",
    "dev:elasticsearch": "elasticsearch",
    "dev:mongodb": "brew services stop mongodb/brew/mongodb-community@6.0 && brew services start mongodb/brew/mongodb-community@6.0",
    "dev:clickhouse": "brew services stop clickhouse && brew services start clickhouse",
    "dev:kibana": "wait-on http://localhost:9200 && brew services stop kibana-full && brew services start kibana-full",
    "stop": "brew services stop elasticsearch",
    "serve": "node dist/index.js",
    "ngrok": "ngrok http 3000",
    "lint": "tsc --noEmit && eslint \"src/**/*.{tsx,ts,js}\"",
    "start-mcp": "nodemon -r dotenv/config --ext ts --exec 'node -r ts-node/register -r tsconfig-paths/register' src/mcp/index.ts",
    "test-mcp": "npx @modelcontextprotocol/inspector node dist/server/src/mcp/index.js",
    "build": "tsc"
  },
